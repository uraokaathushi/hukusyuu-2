
<table class="table">
 <% if @range == "User" %>
 <h2>Users search for <%= @users.search_word %></h2>
 <tbody>
   <% @users.each do |user| %>
    <tr>
    <td><%= user.name %></td>
    </tr>
    <% end %>
 </tbody>
   <% else %>
   <h2>Books search for <%= @books.search_word %></h2>
   <tbody>
      <% @books.each do |book| %>
        <tr>
          <td><%= book.title %></td>
          <td><%= book.body %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
 </table>
